{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "DynamoDB": {
    "ServiceURL": "http://localhost:8000",
    "AuthenticationRegion": "eu-central-1",
    "AccessKey": "dev",
    "SecretKey": "dev"
  },

  "RateLimiting": {
    "Policy": "token",
    "TokenLimit": 100,
    "ReplenishmentPeriodSeconds": 10,
    "TokensPerPeriod": 20,
    "QueueLimit": 2
  }
}
